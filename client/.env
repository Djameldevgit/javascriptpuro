# Evitar chequeos de precompilación en producción
SKIP_PREFLIGHT_CHECK=true

# URL base de la API en producción
REACT_APP_API_URL=https://servidorrender.onrender.com

# Entorno actual, importante para configurar Axios y otras bibliotecas
REACT_APP_NODE_ENV=production

# Clave secreta para otras configuraciones opcionales
REACT_APP_SECRET_KEY=tu_clave_secreta_produccion

# Configuración de Google Analytics (si lo utilizas)
REACT_APP_GOOGLE_ANALYTICS_ID=TU_GOOGLE_ANALYTICS_ID

# Otras configuraciones de servicios en producción (opcional)
# Ejemplo: REACT_APP_FIREBASE_API_KEY=tu_firebase_key
